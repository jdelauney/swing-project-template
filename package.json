{
  "name": "swing-css-reset",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:silent": "vitest --silent --passWithNoTests",
    "test:ui": "vitest run --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint:js": "eslint src --fix",
    "lint:scss": "pnpx stylelint **/*.scss --max-warnings 0 --fix",
    "sonarlint": "sonarlint --src 'src/**/*' --exclude 'src/**/__tests__/*' --html-report 'reports/sonarlint-report.html'",
    "lint:all": "pnpm lint:js && pnpm lint:scss && pnpm sonarlint",
    "lint": "pnpm lint:all",
    "format": "prettier --write",
    "prepare": "husky install",
    "scss:sorter": "postcss --config ./src ./src/**/*.scss --replace"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "postcss": "^8.4.24",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-scss": "^4.0.6",
    "postcss-sort-media-queries": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/eslint": "^8.40.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lightningcss": "^1.21.1",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sonarlint": "^1.1.0",
    "stylelint": "^15.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "vite": "^4.3.9",
    "vite-plugin-lightningcss": "^0.0.5",
    "vitest": "^0.32.2"
  }
